{
  "name": "pvonweb-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node generateConfig.mjs --dev && react-scripts start",
    "build-dev": "node generateConfig.mjs --dev && react-scripts build",
    "build-test": "node generateConfig.mjs --test && react-scripts build",
    "build-staging": "node generateConfig.mjs --staging && react-scripts build",
    "build-prod": "node generateConfig.mjs --prod && react-scripts build",
    "test": "react-scripts test",
    "test-e2e": "cypress run",
    "global-start": "cd ../ && npm start",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.8",
    "@mui/x-data-grid": "^5.17.26",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^18.13.0",
    "@types/react-grid-layout": "^1.3.2",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-grid-layout": "^1.3.4",
    "react-router-dom": "^6.7.0",
    "react-scripts": "5.0.1",
    "socket.io-client": "^4.5.4",
    "typescript": "^5.0.2",
    "web-vitals": "^2.1.4",
    "yup": "^1.0.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/luxon": "^3.3.0",
    "cypress": "^12.9.0",
    "eslint": "^8.33.0"
  }
}
